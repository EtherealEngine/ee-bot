{
  "name": "XREngine-Bot",
  "version": "0.5.8",
  "repository": {
    "type": "git",
    "url": "git://github.com/XRFoundation/XREngine.git"
  },
  "engines": {
    "node": ">= 16.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmClient": "npm",
  "main": "src/bot.ts",
  "scripts": {
    "check-errors": "tsc --noemit",
    "build-publish": "rimraf dist && cti entrypoint ./src -o index.ts && cross-env APP_ENV=production rollup -c",
    "validate": "npm run test"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "puppeteer": "19.2.0",
    "puppeteer-select": "^1.0.3",
    "webxr-emulator": "git+https://github.com/XRFoundation/WebXR-emulator-extension.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/expect-puppeteer": "^5.0.3"
  }
}
