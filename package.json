{
  "name": "ee-bot",
  "version": "0.5.8",
  "description": "A test bot using puppeteer",
  "repository": {
    "type": "git",
    "url": "git://github.com/etherealengine/etherealengine.git"
  },
  "etherealEngine": {
    "version": "1.1.0"
  },
  "engines": {
    "node": ">= 16.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmClient": "npm",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node --swc ./src/index.ts",
    "dev-koa": "ts-node --swc ./src/index_koa.ts",
    "build": "tsc",
    "check-errors": "tsc --noemit",
    "validate": "npm run test",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "precommit": "no-master-commits -b master"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-router": "^12.0.0",
    "puppeteer": "^19.6.3",
    "ts-node": "10.9.1",
    "webxr-emulator": "etherealengine/WebXR-emulator-extension"
  },
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "1.3.41",
    "@types/expect-puppeteer": "^5.0.3",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "@types/mocha": "^10.0.1"
  }
}
